<h4 class="field-title"><%= title %></h4>
<textarea class='wysihtml5-<%= unique_key %> form-control' placeholder="Enter text" style="width: 100%; height: 300px;" name="<%= name %>"><%= value %></textarea>
<br>

<script type="text/javascript">
  $(document).ready(function(){

    $('.wysihtml5-<%= unique_key %>').each(function(i, elem) {
      $(elem).wysihtml5();
    });

  })
</script>
